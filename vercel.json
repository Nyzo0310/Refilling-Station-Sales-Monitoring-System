{
    "version": 2,
    "framework": null,
    "outputDirectory": "public",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2",
            "includeFiles": "{app,bootstrap,config,database,lang,public,resources,routes,storage,vendor}/**,isrgrootx1.pem"
        }
    },
    "routes": [
        {
            "src": "/(build|css|js|images|favicon.ico|robots.txt)(.*)",
            "dest": "/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}